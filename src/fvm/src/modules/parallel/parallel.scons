Import('env')

src = [
    'PartMesh.cpp',
    ]

deps = ['rlog', 'fvmbase',  'openmpi']
env.createSharedLibrary('fvmparallel',src, deps)

deps += ['fvmparallel']

env.createSwigModule('fvmparallel',sources=['PartMesh.i'],deplibs=deps)
                     
deps += ['importers', 'exporters']

env.createExe('pFVM',['pFVM.cpp'], deps)
